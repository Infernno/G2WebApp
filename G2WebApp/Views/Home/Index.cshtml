@inherits Nancy.ViewEngines.Razor.NancyRazorViewBase<System.Collections.Generic.List<G2WebApp.Core.Data.ViewModels.StoryViewModel>>

@if (Model.Count == 0)
{
    <p align="center" class="center-message">Нет ничего нового :(</p>
}
else
{
    foreach (var post in Model)
    {
        @Html.Partial("Views/Shared/Post", post)
    }
}